@model Cadastro

@{
    ViewData["Title"] = "Cadastro";
}

<div class="titulo_pagina">
    <h1>Cadastro de perfil</h1>
</div>

<form method="POST" action='@Url.Action("Cadastrar")' enctype="multipart/form-data" class="cadastro">
    <div class="inputCadastro">
        <label for="Email">Email</label>
        <input type="email" name="Email" id="Email" />
    </div>

    <div class="inputCadastro">            
        <label for="Nome">Nome</label>
        <input type="text" name="Nome" id="Nome" />
    </div>

    <div class="inputCadastro">
        <label for="Username">Nome de usu√°rio</label>
        <input type="text" name="Username" for="Username" />
    </div>

    <div class="inputCadastro">
        <label for="Senha">Senha</label>
        <input type="password" name="Senha" for="Senha" />
    </div>
    
    <button class="buttonCadastro" type="submit">Cadastrar</button>
</form>
